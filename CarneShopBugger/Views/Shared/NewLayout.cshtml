    <link href="~/Carneshop/assets/css/flaticon.css" rel="stylesheet">
    <link href="~/Carneshop/assets/css/owl.css" rel="stylesheet">
    <link href="~/Carneshop/assets/css/bootstrap.css" rel="stylesheet">
    <link href="~/Carneshop/assets/css/jquery.fancybox.min.css" rel="stylesheet">
    <link href="~/Carneshop/assets/css/animate.css" rel="stylesheet">
    <link href="~/Carneshop/assets/css/color.css" rel="stylesheet">
    <link href="~/Carneshop/assets/css/style.css" rel="stylesheet">
